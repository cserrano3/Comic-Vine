{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es2015",
        "target": "es5",
        "jsx": "react",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "rootDir": "./src",
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules"
    ],
    "types": [
        "node"
    ]
}